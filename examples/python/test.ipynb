    "from qiskit import QuantumCircuit\n",
    "import qiskit.quantum_info as qi\n",
    "\n",
    "# Create circuit with gate that modifies Pauli phase\n",
    "qc = QuantumCircuit(2)\n",
    "qc.cz(0, 1)\n",
    "\n",
    "# Generate random Pauli\n",
    "pauli = qi.random_pauli(2, seed=123)\n",
    "\n",
    "# Try and evolve\n",
    "pauli.evolve(qc)"
   ]
  }
